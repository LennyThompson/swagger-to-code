{
  "swagger": [
    "swagger.yaml"
  ],
  "templates": [
    {
      "template": "generateCPPHeaderFromComponent",
      "file-extension": ".h",
      "path-root": "CppRoot",
      "path": "comp/include",
      "target": "Each",
      "generate-type": "cpp",
      "generate-params": ["component"],
      "use": true
    }
  ],
  "root-paths": [
    {
      "name": "CppRoot",
      "path": "cpp/"
    },
    {
      "name": "Template",
      "path": "templates/"
    },
    {
      "name": "Script",
      "path": "scripts/"
    },
    {
      "name": "Swagger",
      "path": "scripts/"
    }
  ],
  "meta-data":  ""
}